{
	"name": "video_pause",
	"version": "1.0.0",
	"private": false,
	"description": "暫停不在view中的影片",
	"author": "Exinfinite Chen <chen.min.home@gmail.com>",
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not ie <= 10"
	],
	"engines": {
		"node": ">= 10.11.0",
		"npm": ">= 5.6.0",
		"yarn": ">= 1.6.0"
	},
	"main": "./src/main.ts",
	"scripts": {
		"build": "set NODE_ENV=prod&&webpack --mode production",
		"dev": "set NODE_ENV=dev&&webpack --mode development",
		"type-check": "tsc --noEmit",
		"type-check:watch": "npm run type-check -- --watch",
		"start:watch": "npm run dev -- --watch",
		"start:serve": "webpack-dev-server",
		"start": "concurrently npm:start:*",
		"release": "standard-version --header=\"# Changelog \"",
    	"release-first": "npm run release -- --first-release",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"devDependencies": {
		"@babel/core": "^7.10.4",
		"@babel/polyfill": "^7.10.4",
		"@babel/preset-env": "^7.10.4",
		"babel-loader": "^8.3.0",
		"concurrently": "^5.2.0",
		"intersection-observer": "^0.11.0",
		"terser-webpack-plugin": "^3.0.6",
		"ts-loader": "^9.4.2",
		"typescript": "^3.9.6",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1",
		"webpack-dev-server": "^3.11.0"
	},
	"dependencies": {}
}
